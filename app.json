{
  "expo": {
    "name": "Dene",
    "slug": "dene",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/4ry1337/dene",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dene",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ff6900"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ff6900"
        }
      ],
      "expo-sqlite",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Noto_Sans_Mono/NotoSansMono-VariableFont_wdth,wght.ttf"
          ]
        }
      ],
      "expo-web-browser",
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
